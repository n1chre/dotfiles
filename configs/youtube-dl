--ignore-errors
--mark-watched

# Filesystem options
--output '%(title)s.%(ext)s'
--restrict-filenames  # ascii only
--no-mtime

--user-agent 'Mozilla/5.0 (compatible; MSIE 9.0; Windows NT 6.1; Trident/5.0)'

--extract-audio
--audio-format mp3
--format bestaudio

--embed-thumbnail

--prefer-ffmpeg
--hls-prefer-ffmpeg
